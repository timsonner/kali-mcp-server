{
  "mcpServers": {
    "Gemini-Kali-MCP-Server": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--privileged",
        "--network",
        "host",
        "-v",
        "kali_mcp_data:/var/lib/docker",
        "kali-mcp-gemini"
      ],
      "trust": true
    }
  }
}